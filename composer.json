{
  "name": "dorsetdigital/silverstripe-core-theme-functions",
  "version": "1",
  "description": "Core functions for Dorset Digital SilverStripe themes",
  "type": "silverstripe-vendormodule",
  "keywords": ["silverstripe", "theme", "bootstrap", "bootstrap 4"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Tim Burt, Dorset Digital Ltd",
      "role": "Developer",
      "homepage": "https://dorset-digital.net"
    }
  ],
  "require": {
    "composer/installers": "*",
    "silverstripe/framework": "^4.0",
    "dorsetdigital/silverstripe-requirementsinline": "^1",
    "tractorcow/silverstripe-colorpicker": "^4"
  },
  "minimum-stability": "dev",
  "suggest": {
    "https://github.com/DorsetDigital/silverstripe-devwarning": "Adds reminders to the CMS for basic security issues"
  },
  "autoload": {
    "psr-4": {
      "DorsetDigital\\Themes\\Core\\": "src/"
    }
  }
}
